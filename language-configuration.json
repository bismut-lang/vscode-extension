{
  "comments": {
    "lineComment": "#",
    "blockComment": null
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "'", "close": "'", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(def|class|struct|interface|enum|if|while|for)\\b",
      "end": "^\\s*end\\b"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(def|class|struct|interface|enum|if|elif|else|while|for)\\b.*$",
    "decreaseIndentPattern": "^\\s*(end|elif|else)\\b"
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
